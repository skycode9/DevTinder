{
  "name": "devtinder-fullstack",
  "version": "1.0.0",
  "description": "DevTinder - Full Stack Application",
  "main": "server/src/app.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "server": "nodemon server/src/app.js",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "install-all": "npm install && npm run install-client",
    "build": "npm run install-client && npm run build --prefix client",
    "start": "node server/src/app.js",
    "render-build": "npm install && cd client && npm install && npm run build"
  },
  "keywords": [
    "devtinder",
    "mern",
    "dating-app"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "concurrently": "^8.2.2"
  }
}
